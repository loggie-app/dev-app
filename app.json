{
  "expo": {
    "name": "Loggie",
    "slug": "loggie-dev",
    "version": "1.0.14",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.loggie.ios",
      "buildNumber": "1.0.14",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes and upload photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-avatar.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.loggie.android"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "45531b06-e0a1-4e2f-bb70-2bd87b89980c"
      }
    },
    "owner": "splatapps",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ]
  }
}
